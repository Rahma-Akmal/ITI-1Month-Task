@model Course
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Course Details</title>
</head>
<body>
    <h2> <b>Name</b> @Model.CourseName</h2>

    <p> <b>Hours </b> @Model.CourseDuration</p>

    <h3>Students</h3>
    <p> StdName   StdAge    StdDeptID    StdCrsId</p>
    @if (Model.Students != null)
    {
        <ul>
           
            @foreach (var student in Model.Students)



            {

                <li>@student.Name - @student.Age - @student.DeptId - @student.CourseId</li>
            }
        </ul>
    }
    else
    {
        <p>No students are enrolled in this Course.</p>
    }

</body>
</html>